OPENAI_API_KEY=sk-your-key
PORT=3001

# Supabase (for file storage: uploads, stems, MIDI)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# RunPod (optional: use GPU endpoint instead of local Python)
RUNPOD_ENDPOINT_ID=your-endpoint-id
RUNPOD_API_KEY=your-runpod-api-key

# Planner (default: OpenAI). Set runpod_vllm to use RunPod vLLM endpoint
PLANNER_PROVIDER=openai
PLANNER_MODEL=gpt-4o-mini
# RunPod planner (when PLANNER_PROVIDER=runpod_vllm)
PLANNER_RUNPOD_ENDPOINT_ID=your-planner-endpoint-id
PLANNER_RUNPOD_API_KEY=your-runpod-api-key
# Optional: override full URL (e.g. https://api.runpod.ai/v2/xxx)
PLANNER_BASE_URL=